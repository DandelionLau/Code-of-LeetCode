class Solution:
    def longestPalindrome(self, s: str):
        n = len(s)
        dp = [([False] * n) for i in range(n)]
        maxlen = 0
        result = s[0]
        for i in range(n):
            for j in range(i):
                if s[i] == s[j] and (j >= i - 2 or dp[i+1][j-1]):
                    dp[i][j] = True
                    if i-j+1>maxlen:
                        result = s[j:i+1]
                        maxlen = i-j+1
        return result
